{
  "name": "tfs",
  "version": "0.0.0",
  "description": "Share files without leaving your terminal",
  "author": "Olatayo Olayemi <olatayo.olayemi.peter@gmail.com>",
  "bin": "./bin/run",
  "keywords": [
    "tfs"
  ],
  "homepage": "https://github.com/Ownage-FDT/tfs-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Ownage-FDT/tfs-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "commit": "git-cz",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "build": "shx rm -rf dist && tsc -b",
    "format": "prettier --write \"src/**/*.+(ts|js)\" \"test/**/*.+(ts|js)\"",
    "lint": "eslint . --ext .ts --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^2.15.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-update": "^3.1.28",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "crypto-js": "^4.1.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@oclif/test": "^2.3.25",
    "@types/chai": "^4",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/sinon": "^10.0.15",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "chai": "^4.3.7",
    "commitizen": "^4.3.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "mockfs": "^0.1.5",
    "oclif": "^3",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "oclif": {
    "bin": "tfs",
    "dirname": "tfs",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Manage the configuration for tfs"
      },
      "config:set": {
        "description": "Set a configuration value for tfs"
      }
    }
  },
  "engines": {
    "node": ">=16.20.0"
  },
  "bugs": "https://github.com/Ownage-FDT/tfs-cli/issues",
  "types": "dist/index.d.ts",
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "prettier --write --ignore-unknown",
      "eslint --cache --fix"
    ]
  }
}
