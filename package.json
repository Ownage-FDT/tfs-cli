{
  "name": "@ownage-fdt/tfs-cli",
  "version": "0.5.0",
  "description": "Share files without leaving your terminal",
  "author": "Olatayo Olayemi <olatayo.olayemi.peter@gmail.com>",
  "bin": {
    "tfs": "./bin/run"
  },
  "keywords": [
    "tfs"
  ],
  "homepage": "https://github.com/Ownage-FDT/tfs-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Ownage-FDT/tfs-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md",
    ".npmrc"
  ],
  "scripts": {
    "commit": "git-cz",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "build": "shx rm -rf dist && tsc --build --force",
    "format": "prettier --write \"src/**/*.+(ts|js)\" \"test/**/*.+(ts|js)\"",
    "lint": "eslint . --ext .ts --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^2.15.0",
    "@oclif/plugin-autocomplete": "^2.3.8",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.4.1",
    "@oclif/plugin-update": "^3.2.3",
    "@oclif/plugin-warn-if-update-available": "^2.1.0",
    "axios": "^1.5.0",
    "chalk": "^4.1.2",
    "crypto-js": "^4.1.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@oclif/test": "^2.5.4",
    "@types/chai": "^4",
    "@types/crypto-js": "^4.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.17.1",
    "@types/sinon": "^10.0.16",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "chai": "^4.3.8",
    "commitizen": "^4.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "mockfs": "^0.1.5",
    "oclif": "^3",
    "prettier": "^3.0.3",
    "shx": "^0.3.4",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "oclif": {
    "bin": "tfs",
    "dirname": "tfs",
    "scope": "ownage-fdt",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "binAlias": [
      "tfs"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Manage the configuration for tfs"
      },
      "config:set": {
        "description": "Set a configuration value for tfs"
      }
    },
    "update": {
      "s3": {
        "bucket": "tfs-store",
        "folder": "media/cli",
        "host": "https://sgp1.digitaloceanspaces.com"
      }
    },
    "macos": {
      "identifier": "com.trytfs.cli"
    },
    "windows": {
      "name": "TFS CLI"
    }
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "bugs": "https://github.com/Ownage-FDT/tfs-cli/issues",
  "types": "dist/index.d.ts",
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "prettier --write --ignore-unknown",
      "eslint --cache --fix"
    ]
  }
}
