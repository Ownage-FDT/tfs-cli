{
  "name": "@ownage/tfs",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Olatayo Olayemi <olatayo.olayemi.peter@gmail.com>",
  "bin": {
    "tfs": "./bin/run"
  },
  "keywords": [
    "tfs"
  ],
  "homepage": "https://github.com/Ownage-FDT/tfs-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Ownage-FDT/tfs-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "commit": "git-cz",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@oclif/test": "^2.4.2",
    "@types/chai": "^4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.36",
    "chai": "^4",
    "commitizen": "^4.3.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mocha": "^10",
    "oclif": "^3",
    "pnpm": "^8.6.3",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "oclif": {
    "bin": "tfs",
    "dirname": "tfs",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Ownage-FDT/tfs-cli/issues",
  "types": "dist/index.d.ts",
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": "eslint --cache --fix"
  }
}
