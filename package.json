{
  "name": "tfs",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Olatayo Olayemi <olatayo.olayemi.peter@gmail.com>",
  "bin": {
    "tfs": "./bin/run"
  },
  "keywords": [
    "tfs"
  ],
  "homepage": "https://github.com/Ownage-FDT/tfs-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Ownage-FDT/tfs-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "commit": "git-cz",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "build": "shx rm -rf dist && tsc -b",
    "format": "prettier --write \"src/**/*.+(ts|js)\" \"test/**/*.+(ts|js)\"",
    "lint": "eslint . --ext .ts --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^2.8.7",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.1.3",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@oclif/test": "^2.3.23",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^16.18.36",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "chai": "^4",
    "commitizen": "^4.3.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "oclif": "^3",
    "pnpm": "^8.6.3",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "tfs",
    "dirname": "tfs",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": ":",
    "topics": {
      "auth": {
        "description": "Manage authentication."
      }
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/Ownage-FDT/tfs-cli/issues",
  "types": "dist/index.d.ts",
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "prettier --write --ignore-unknown",
      "eslint --cache --fix"
    ]
  }
}